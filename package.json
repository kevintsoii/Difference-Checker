{
  "name": "difference-checker",
  "private": true,
  "main": "electron/main.js",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "dev": "concurrently -k \"vite\" \"wait-on tcp:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .\"",
    "build": "vite build && electron-builder",
    "preview": "vite preview"
  },
  "build": {
    "appId": "com.example.difference-checker",
    "files": [
      "dist/**",
      "electron/**"
    ],
    "extraMetadata": {
      "main": "electron/main.js"
    },
    "mac": {
      "icon": "electron/assets/logo.icns"
    },
    "win": {
      "icon": "electron/assets/logo.ico"
    },
    "linux": {
      "icon": "electron/assets/logo.png"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@reduxjs/toolkit": "^2.9.0",
    "@tailwindcss/vite": "^4.1.13",
    "electron-store": "^10.1.0",
    "framer-motion": "^12.23.12",
    "react": "^19.1.1",
    "react-codemirror-merge": "^4.25.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.1",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.1.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.1.2",
    "wait-on": "^8.0.5"
  }
}
